# Name of host running the TEPS
# teps.server.name=LOCALHOST

# Port on which the TEPS is listening
# teps.server.port=1920

# TEPS login user name
# username=sysadmin

# TEPS login password
# user.password=apassword


# IP address of agent to transfer to / from 
# os.agent=129.196.130.225


# Transfer Action [ GET / PUT ]
# transfer.action=GET

# Source directory containing the file(s) 
# src.dir = %NCHOME%\\omnibus\\probes\\win32
# src.dir = $NCHOME/omnibus/probes/ARCH


# Target directory to recieve the file(s)
# tgt.dir=c:\\temp
# tgt.dir=/tmp

# comma separated list of files to transfer
# file.name=simnet.props,simnet.rules,simnet.def

# controls if file transfer can be out side of NCHOME [true / false : limit the transfer to from within NCHOME]
# deresricted=true


