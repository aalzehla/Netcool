<!--  
This command file creates a relationship definition for the IBM Tivoli Netcool/OMNIbus
root cause/symptoms.
-->

<methodCall xmlns:relationship="http://www.ibm.com/tivoli/netcool/webtop/relationships/7.4">

	<method methodName="relationship.createRelationship">
		<relationship:relationship name="nco_root_cause"
				displayName="IBM Tivoli Netcool/OMNIbus Root Cause/Symptom"
				displayNameKey="nco_root_cause.displayname"
				description="Show root cause/symptom relationships generated by IBM Tivoli Netcool/OMNIbus."
				descriptionKey="nco_root_cause.description">
			<relationship:definition type="relationshipColumn">
				<relationship:relationshipColumn column="ParentIdentifier" keyColumn="Identifier"/>
			</relationship:definition>
		</relationship:relationship>
	</method>

</methodCall>
