-- part of the history group

tokenRingHistoryTable OBJECT-TYPE
    SYNTAX SEQUENCE OF TokenRingHistoryEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
	"A list of Token Ring statistics entries."
    ::= { history 3 }

tokenRingHistoryEntry OBJECT-TYPE
    SYNTAX TokenRingHistoryEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
	"A collection of statistics kept for a particular
	Token Ring interface."
    INDEX { tokenRingHistoryIndex }
    ::= { tokenRingHistoryTable 1 }

TokenRingHistoryEntry ::= SEQUENCE {
  tokenRingHistoryIndex                   INTEGER,
  tokenRingHistorySampleIndex		  INTEGER,
  tokenRingHistoryIntervalStart		  TimeTicks,
  tokenRingHistoryDropEvents              Counter,
  tokenRingHistoryOctets                  Counter,
  tokenRingHistoryPackets                 Counter,
  tokenRingHistoryDataBroadcastPackets    Counter,
  tokenRingHistoryDataAllRoutesBroadcasts Counter,
  tokenRingHistoryDataMulticastPackets    Counter,
  tokenRingHistoryMacOctets               Counter,
  tokenRingHistoryMacPackets              Counter,
  tokenRingHistoryRingPurgeEvents         Counter,
  tokenRingHistoryRingPurgePackets        Counter,
  tokenRingHistoryBeaconEvents            Counter,
  tokenRingHistoryBeaconPackets           Counter,
  tokenRingHistoryMonitorContentionEvents Counter,
  tokenRingHistoryMonitorContentionPackets Counter,
  tokenRingHistoryNAUNChanges             Counter,
  tokenRingHistoryLineErrors              Counter,
  tokenRingHistoryInternalErrors          Counter,
  tokenRingHistoryBurstErrors             Counter,
  tokenRingHistoryACErrors                Counter,
  tokenRingHistoryAbortErrors             Counter,
  tokenRingHistoryLostFramesErrors        Counter,
  tokenRingHistoryCongestionErrors        Counter,
  tokenRingHistoryFrameCopiedErrors       Counter,
  tokenRingHistoryFrequencyErrors         Counter,
  tokenRingHistoryTokenErrors             Counter,
  tokenRingHistorySoftErrorReports        Counter,
  tokenRingHistoryActiveStations	  INTEGER
}

tokenRingHistoryIndex OBJECT-TYPE
    SYNTAX INTEGER (1..65535)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
	"The history of which this entry is a part.  The
	history identified by a particular value of this
	index is the same history as identified
	by the same value of historyControlIndex."
    ::= { tokenRingHistoryEntry 1 }

tokenRingHistorySampleIndex OBJECT-TYPE
    SYNTAX OBJECT IDENTIFIER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
	"An index that uniquely identifies the particular
	sample this entry represents among all samples
	associated with the same historyControlEntry.
	This index starts at 1 and increases by one
	as each new sample is taken."
    ::= { tokenRingHistoryEntry 2 }

tokenRingHistoryIntervalStart OBJECT-TYPE
    SYNTAX TimeTicks
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
	"The value of sysUpTime at the start of the interval
	over which this sample was measured.  If the probe
	keeps track of the time of day, it should start
	the first sample of the history at a time such that
	when the next hour of the day begins, a sample is
	started at that instant.  Note that following this
	rule may require the probe to delay collecting the
	first sample of the history, as each sample must be
	of the same interval.  Also note that the sample which
	is currently being collected is not accessible in this
	table until the end of its interval."
    ::= { tokenRingHistoryEntry 3 }

tokenRingHistoryDropEvents OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
	"The total number of events in which packets
	were dropped by the probe due to lack of resources
	during this interval.  Note that this number is not
	necessarily the number of packets dropped, it is just
	the number of times this condition has been detected."
    ::= { tokenRingHistoryEntry 4 }

tokenRingHistoryOctets OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The total number of octets of data (including
        those in bad packets) received on the
        network (excluding framing bits but including
        FCS octets)."
    ::= { tokenRingHistoryEntry 5 }

tokenRingHistoryPkts OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The total number of packets (including error packets)
        received during this sampling interval."
    ::= { tokenRingHistoryEntry 6 }

tokenRingHistoryDataBroadcastPkts OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The total number of good Data packets received during
	this sampling interval that were directed to an LLC
	broadcast address (0xFFFFFFFFFFFF or 0xC000000000FF)."
    ::= { tokenRingHistoryEntry 7 }

tokenRingHistoryDataAllRoutesBroadcastPkts OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The total number of good packets received during this
	sampling interval that were All Routes Broadcast."
    ::= { tokenRingHistoryEntry 8 }

tokenRingHistoryDataMulticastPkts OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The total number of good Data packets received during this
	sampling interval that were directed to a local or global
	multicast or functional address.  Note that this number does
	not include packets directed to	the broadcast address."
    ::= { tokenRingHistoryEntry 9 }

tokenRingHistoryMACOctets OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The total number of octets of data in MAC packets
	(including those in bad MAC packets) received on the
